<div class="container-fluid bg-dark-subtle">
    <div class="container">
    <footer class="d-flex flex-wrap justify-content-between align-items-center  border-top">
      <p class="col-md-4 mb-0 text-body-secondary">Â© {{ 'now' | date('Y') }} Rebecca Luff</p>
  
      <a href="/" class="col-md-4 d-flex align-items-center justify-content-center mb-3 mb-md-0 me-md-auto link-body-emphasis text-decoration-none">
        <svg class="bi me-2" width="40" height="32"><use xlink:href="#bootstrap"></use></svg>
      </a>
  
      <ul class="nav col-md-4 justify-content-end">
        {% set menu = craft.entries.section('menu').all() %}


        {% for entry in menu %}
        {% if entry.entryLink.one()|length %}
            {% set navlink = entry.entryLink.one().url %}
          {% else %}
      
            {% set navlink = alias('@web') ~ '/' ~ entry.customLink %}
        {% endif %}
        {% set btnnav = "" %}
        {% set mnuClass = "" %}
       
        <li class="nav-item {{ macros.activeLinkClass(navlink, entry.level)|trim }} level{{ entry.level }} {{ entry.title |lower }}">
          <a id="{{ entry.title }}" href="{{ navlink }}" class="nav-link {{ macros.activeLinkClass(navlink, entry.level) |trim }} level{{ entry.level }} {{ mnuClass }}">{{entry.title }}</a>
        </li>
       
        {% endfor %} </ul>
    </footer>
  </div>
