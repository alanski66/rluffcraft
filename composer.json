{
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "canonical": false,
      "type": "composer",
      "url": "https://composer.craftcms.com"
    }
  ],
  "require": {
    "cloudgrayau/cleantalk": "1.1.0",
    "craftcms/ckeditor": "4.9.0",
    "craftcms/cms": "5.7.7",
    "ether/seo": "5.0.0",
    "ether/splash": "5.0.0",
    "leowebguy/device-detect": "3.1",
    "mmikkel/cp-field-inspect": "2.0.4",
    "nystudio107/craft-minify": "5.0.0",
    "spicyweb/craft-embedded-assets": "5.4.3",
    "verbb/buttonbox": "5.0.0",
    "vlucas/phpdotenv": "^5.4.0",
    "wrav/oembed": "3.1.5",
    "xpertbot/craft-wheelform": "4.0.3"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}